{
    "name": "gigabyte-software/nova-map-field",
    "description": "Map Field for Laravel Nova. Updating to include multi-point field",
    "keywords": [
        "laravel",
        "map",
        "map marker",
        "point",
        "polygon",
        "multi polygon",
        "spatial",
        "openlayers",
        "geocoder",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "laravel/nova": "^4.0",
        "matanyadaev/laravel-eloquent-spatial": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Mostafaznv\\NovaMapField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mostafaznv\\NovaMapField\\NovaMapFieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
